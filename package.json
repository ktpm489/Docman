  "bugs": {
    "url": "https://github.com/seyi-adeleke/Docman/issues"
  },
  "homepage": "https://github.com/seyi-adeleke/Docman#readme",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "coveralls": "^2.13.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-coveralls": "^0.1.4",
    "gulp-exit": "0.0.2",
    "gulp-inject-modules": "^1.0.0",
    "gulp-istanbul": "^1.1.2",
    "gulp-jasmine-node": "^1.0.7",
    "gulp-nodemon": "^2.2.1",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "node-localstorage": "^1.3.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.2.1"
